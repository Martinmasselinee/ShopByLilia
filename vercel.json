{
  "functions": {
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 30
    },
    "app/api/auth/register/route.ts": {
      "maxDuration": 30
    },
    "app/api/users/route.ts": {
      "maxDuration": 15
    },
    "app/api/notifications/route.ts": {
      "maxDuration": 15
    },
    "app/api/**/*.ts": {
      "maxDuration": 20
    }
  },
  "env": {
    "PRISMA_CLIENT_ENGINE_TYPE": "dataproxy"
  }
}

